{
  "cli": {
    "version": ">= 16.3.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "environment": "development",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://good-cup-api.vercel.app"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "environment": "preview",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://good-cup-api.vercel.app"
      }
    },
    "production": {
      "ios": {
        "distribution": "store"
      },
      "autoIncrement": true,
      "environment": "production",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://good-cup-api.vercel.app"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
