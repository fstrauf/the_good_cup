    "@rn-primitives/tooltip": "~1.1.0",
    "@rn-primitives/types": "~1.1.0",
    "@vercel/node": "^5.1.14",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "eas-cli": "^16.3.1",
    "expo": "^52.0.46",
    "expo-auth-session": "^6.0.3",
    "expo-blur": "^14.0.3",
