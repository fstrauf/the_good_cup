    "expo-web-browser": "^14.0.2",
    "lucide-react-native": "^0.378.0",
    "nativewind": "^4.1.23",
    "openai": "^4.93.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-reanimated": "~3.16.7",
